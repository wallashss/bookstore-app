{
  "name": "bookstore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "launch": "npm run db:backup && npm run db:migrate-up-prod && npm run start",
    "start": "node -r dotenv/config dist/src/main/index.js",
    "build": "tsc",
    "dev": "nodemon src/main/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:backup-dev": "ts-node -r dotenv/config db/backup.ts",
    "db:backup": "node -r dotenv/config dist/db/backup.js",
    "db:update": "node -r dotenv/config db/update.js && knex --knexfile db/ingest.js migrate:latest",
    "db:update-dev": "ts-node -r dotenv/config db/update.ts && knex --knexfile db/ingest.ts migrate:latest",
    "db:fix-migrations-dev": "ts-node -r dotenv/config db/fix-migrations",
    "db:fix-migrations": "node -r dotenv/config dist/db/fix-migrations",
    "db:migrate-new": "knex --knexfile db/knexfile.ts migrate:make -x ts",
    "db:migrate-up": "knex --knexfile db/knexfile.ts migrate:latest",
    "db:migrate-up-prod": "npm run db:fix-migrations && knex --knexfile db/knexfile.js migrate:latest",
    "db:migrate-down": "knex --knexfile db/knexfile.ts migrate:rollback",
    "db:migrate-list": "knex --knexfile db/knexfile.ts migrate:list"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/diacritics": "^1.3.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.5",
    "@types/pdfkit": "^0.12.3",
    "@types/pdfmake": "^0.1.19",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1058.0",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "csv": "^6.0.5",
    "diacritics": "^1.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "knex": "^0.95.15",
    "node-html-parser": "^5.2.0",
    "pdfkit": "^0.13.0",
    "pdfkit-table": "^0.1.73",
    "pdfmake": "^0.2.4",
    "sqlite3": "^5.0.2"
  }
}
